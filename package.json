  },
  "repository": {
    "type": "git",
    "url": "git@github.com:leebyron/spec-md.git"
  },
  "bin": "./bin/spec-md",
  "main": "./src/index.js",
  "scripts": {
    "test": "exit 0",
    "build": "mkdir -p out; cp css/* out; ./bin/spec-md README.md > out/index.html",
    "predeploy": "npm run build",
    "deploy": "(cd out && git init && git config user.name \"Travis CI\" && git config user.email \"lee@leebyron.com\" && git add . && git commit -m \"Deploy to GitHub Pages\" && git push --force --quiet \"https://${GH_TOKEN}@github.com/leebyron/spec-md.git\" master:gh-pages > /dev/null 2>1)"
  },
  "dependencies": {
    "bluebird": "~2.9.0",
    "highlight.js": "^8.4.0",
    "js-beautify": "^1.5.5",
    "pegjs": "~0.8.0",
    "yargs": "^3.6.0"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "files": [
    "bin",
    "css",
    "src",
    "LICENSE"
  ],
  "keywords": [
    "ast",
    "ecma",
    "ecmascript",
    "html",
    "markdown",
    "markup",
    "spec",
    "specs",
    "specification",
    "typography",
    "code"
  ]
}
